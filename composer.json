{
    "name": "lioneagle/le-utils",
    "description": "This is my package LeUtils",
    "keywords": [
        "lioneagle",
        "laravel",
        "le-utils"
    ],
    "homepage": "https://github.com/lioneagle/le-utils",
    "license": "MIT",
    "authors": [
        {
            "name": "James Mooring",
            "email": "james@lioneagle.solutions",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.55"
    },
    "require-dev": {
        "brianium/paratest": "^6.3",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Lioneagle\\LeUtils\\": "src",
            "Lioneagle\\LeUtils\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lioneagle\\LeUtils\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/testbench package:test --parallel --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
