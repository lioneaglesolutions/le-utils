{
    "name": "lioneagle/le-utils",
    "description": "This is my package LeUtils",
    "keywords": [
        "lioneagle",
        "laravel",
        "le-utils"
    ],
    "homepage": "https://github.com/lioneagle/le-utils",
    "license": "MIT",
    "authors": [
        {
            "name": "James",
            "email": "james@lioneagle.solutions",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.55"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.10",
        "brianium/paratest": "^6.3",
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Lioneagle\\LeUtils\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lioneagle\\LeUtils\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
